{
  "printWidth": 150,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "proseWrap": "always",
  "overrides": [
    {
      "files": ["*.tsx", "*.jsx"],
      "options": {
        "printWidth": 120,
        "bracketSameLine": false
      }
    }
  ]
}
